# Some helful aliases to add to your .bash_profile

# Open text editors
alias st="open -a 'Sublime Text'"
alias cda="open -a 'Coda 2'"
alias ia="open -a 'iA Writer'"

# Start local servers
alias svp="python -m SimpleHTTPServer"
alias svphp="php -S localhost:8000"

# Jekyll
alias jek="jekyll serve --baseurl"
alias jekb="jekyll build"
